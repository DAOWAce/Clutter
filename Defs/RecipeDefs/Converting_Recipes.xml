<RecipeDef>
		<defName>MakeSpongeGun</defName>
		<label>Make Sponge Gun</label>
		<description>Build ancient portable extinguisher device.</description>
		<jobString>Converting.</jobString>
		<workAmount>500</workAmount>
		<workTimeSkillNeed>ConvertingTime</workTimeSkillNeed>
		<efficiencySkillNeed>ConvertingEfficiency</efficiencySkillNeed>
		<workEffect>ButcherMechanoid</workEffect>
        <sustainerSoundDef>Recipe_ButcherCorpseMechanoid</sustainerSoundDef>
    <ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>Metal</li>
					</thingDefs>
				</filter>
				<count>30</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Silver</li>
					</thingDefs>
				</filter>
				<count>10</count>
			</li>
		</ingredients>
		<products>
			<li>
				<thingDef>SpongeGun</thingDef>
				<count>1</count>
			</li>
		</products>
		<fixedIngredientFilter>
			<thingDefs>
				<li>Metal</li>
				<li>Silver</li>
			</thingDefs>
		</fixedIngredientFilter>
		<skillGains>
			<li>
				<skill>Crafting</skill>
				<xp>500</xp>
			</li>
		</skillGains>
	</RecipeDef>

